<div class="col-4 places-list">

    <h2>{{this.name}}</h2>

    <p>Difficulty: {{this.difficulty}}</p>

    <p>Ingredients</p>

    <ul>

        {{#each this.ingredients}}

        <li>{{this}}</li>

        {{/each}}

    </ul>

</div>

<div class="col-7 places-list">

    <p>Steps</p>

    <ol>

        {{#each this.steps}}

        <li>{{this}}</li>

        {{/each}}

    </ol>

</div>