<nav>

    <ul>

        <li><a href="/"><img src="/images/chef-burdeos.png" alt="Logo" class="chef"></a></li>

        <li class="title">TartApp</li>

    </ul>

</nav>

<div class="container-flex places">

    <div class="row justify-content-center">

        <div class="col-3 places-list">

            <h2>Places List</h2>

            {{#each places}}

            <ul>

                <li>{{name}}</li>

            </ul>

            {{/each}}


            <a class="hover-btn btn" href="/places/new">Add a place</a>

            <a class="hover-btn btn" href="/welcome">Go back</a>

        </div>

        <div class="col-8">

            <div id="map"></div>

        </div>

    </div>

</div>

<aside class="aside">

    <p>Made for Ironhack Madrid</p>

</aside>




<script src="https://unpkg.com/axios/dist/axios.min.js"></script>
