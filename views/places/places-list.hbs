{{> navBar}}

<div class="container-flex section">

    <div class="row justify-content-center">

        <div class="col-3 section-list">

            <h2>Places List</h2>

            {{#each places}}

            <ul>

                <li>{{name}}</li>

            </ul>

            {{/each}}


            <a class="hover-btn btn" href="/places/new">Add a place</a>

            <a class="hover-btn btn" href="/welcome">Go back</a>

        </div>

        <div class="col-8">

            <div id="map" class="show fadeIn"></div>

        </div>

    </div>

</div>

{{> footer}}




<script src="https://unpkg.com/axios/dist/axios.min.js"></script>
<script src="/javascripts/map-app.js"></script>
<script type="text/javascript"
    src="https://maps.googleapis.com/maps/api/js?key=AIzaSyA6QuEerMaDGdLKWQI5Unhol9Nr2_2HtYY&libraries=places"></script>



